{
  "name": "skelf",
  "version": "0.0.2",
  "description": "Understanding and Modifying Structured Binary Data",
  "main": "dist/index.js",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec 'tsc && node .' -w src/",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "run": "tsc && node .",
    "playground": "nodemon --exec 'tsc && node dist/playground.js' -w src/",
    "playground:run": "tsc && node dist/playground.js"
  },
  "keywords": [
    "binary",
    "blob",
    "parse",
    "parser",
    "encode",
    "encoder",
    "decode",
    "decoder",
    "hex",
    "editor",
    "poke",
    "GNU poke",
    "kaitai",
    "struct",
    "kaitai struct"
  ],
  "author": "oxcl",
  "license": "LGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxcl/skelf.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./space": "./dist/space.js",
    "./read_stream": "./dist/read_stream.js",
    "./write_stream": "./dist/write_stream.js",
    "./data_type": "./dist/data_type.js",
    "./struct": "./dist/struct.js",
    "./types": "./dist/types.js",
    "./errors": "./dist/errors.js",
    "./log": "./dist/log.js",
    "./utils": "./dist/utils.js",
    "./core": "./dist/core/index.js",
    "./common": "./dist/common/index.js",
    "./tools": "./dist/tools.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.3"
  }
}
